import modelo.Planilha;

public class ProcessaPlanilha {
	
	static ArrayList<Planilha> linhas = new ArrayList<>();
	
	private static DAO dao =  new DAO();
	
	
	public static void carregar() throws IOException {
		linhas = dao.abrir();
	}

}

