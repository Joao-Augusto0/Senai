package modelo;

public class Usuarios {
	
	private String email;
	private String senha;
	
	public Usuarios(String email,String senha) {
		this.email =  email;
		this.senha = senha;
	}
	
	public Usuarios(String linha) {
		this.email = linha.split(";")[0];
		this.senha = linha.split(";")[1];

		
	}
	
	
	
}
