		pets.add(new Pet(1,"Cachorro","toto","vira-latas",(float) 26.5,"01/01/2000","rafael","(19)987428374"));
		pets.add(new Pet(1,"Gato","cat","vira-cats",(float) 16.5,"02/03/2005","wyl","(19)987428374"));
