package modelo;

public class Gato extends Animal{

	private int qtdVidas = 7;
	
	public Gato(int qtdVidas,String especie,String nome, float peso,String raça) {
		super(especie,nome,peso,raça);
		this.qtdVidas=qtdVidas;
	}
	@Override
	public String toString() {
		return "Gato [qtdVidas=" + qtdVidas + super.toString() + ", getClass()=" + getClass()
				+ ", hashCode()=" + hashCode() + super.toString() + "]";
	}
	
	
}

