package modelo;

public class Acumula {

	public static void main(String[] args) {
		System.out.print("o algoritimo de acumulação");
		System.out.print("simplesm, retornou =" +);
		System.out.print("o algoritimo de acumulação");
		
	}
	
	public static int simples() {
		int x = 0;
		for(int i = 0;i < 10;i++)
			x+=i;
		return x;
	}

}
