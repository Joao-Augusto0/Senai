package modelo;

public class Acumula {

	public static void main(String[] args) {
		System.out.println("o algoritimo de acumulação");
		System.out.println("simplesm, retornou =");
		System.out.print(simples());
		
	}
	
	public static int simples() {
		int x = 0;
		for(int i = 0;i < 10;i++)
			x+=i;
		return x;
	}

}
