package modelo;

public class Vetores {

	public static void main(String[] args) {

		
	
	}

	public static void mostra() {
		
		int [] vetorInteiro  = new int[5];
		
		vetorInteiro[0] = 2;
		vetorInteiro[1] = 4;
		vetorInteiro[2] = 5;
		vetorInteiro[3] = 6;
		vetorInteiro[4] = 8;
		
		for (int i = 0; i < vetorInteiro.length; i++) {
			System.out.print(i + "\t"+ vetorInteiro[1]);
		}								
	}
}