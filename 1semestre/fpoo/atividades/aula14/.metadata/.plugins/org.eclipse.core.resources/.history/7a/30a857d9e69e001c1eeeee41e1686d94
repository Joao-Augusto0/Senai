package aula14;

import java.util.Scanner;
import java.util.Random;

public class Ex3 {
	public static Scanner leia;
	public static void main(String[] args) {
		leia = new Scanner(System.in);
		
		Random rand = new Random();
		
		for(int i = 0; i < 10; i++) {
	
			int r = rand.nextInt(10);
			System.out.println(r);
		}
		
	}

}
