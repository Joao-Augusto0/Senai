package aula14;

import java.util.Scanner;

public class Ex3 {
	public static Scanner leia;
	public static void main(String[] args) {
		leia = new Scanner(System.in);

		int v [] = new int[10];
		int ma = 0, me = 9;
		
		for(int i = 0; i < v.length;i++) {
			System.out.print("digite o " +(i+1)+" valor");
			v[i] = leia.nextInt();
			
			if(v[i] > ma) {
				ma=v[i];
			}
		}
		
		for(int j = 0; j < v.length;) {
			if(v[j] > me) {
				me = v[j];
			}
		}
		System.out.println("maior  valor  = "+ma);
		System.out.println("maior  valor  = "+me);
	}
}