package aula15;

import java.util.Scanner;

public class Ex02 {
	public static Scanner leia;

	public static void main(String[] args) {

	leia = new Scanner(System.in);
		
	int []m = new int[10];
	
	for(int i = 0; i < 10; i++) {
		System.out.println("digite 10 valores aleatorios: ");
		m[i] = leia.nextInt();
		
		
		}
	}
}