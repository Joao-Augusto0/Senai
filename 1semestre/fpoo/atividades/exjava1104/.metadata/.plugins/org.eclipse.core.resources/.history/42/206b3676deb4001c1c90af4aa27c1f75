package modelo;

import java.util.Date;

public class Cliente extends Pessoa{

	private int id;
	private double creditoLimite;


public void Cliente() {
	return;
}

public Cliente(int id, double creditoLimite,String nomeCliente,String telefoneCliente,Date nascimentoCliente) {
	
	setNome(nomeCliente);
	setTelefone(nascimentoCliente);
	setNascimento(nascimentoCliente);
	this.id = id;
	this.creditoLimite = creditoLimite;
}

public String toString(){
	return "Funcionario: " +getNome() + "telefone: " + getTelefone() + "nascimento: " + getNascimento() + "id: " + id + "creditoLimite: " + creditoLimite;
}

}